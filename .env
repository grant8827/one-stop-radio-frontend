# OneStopRadio Frontend Configuration - DEFAULT
# This will be overridden by .env.local (development) or .env.production (Railway)

# Default backend URLs (will be overridden)
REACT_APP_API_BASE_URL=http://localhost:8002
REACT_APP_SIGNALING_URL=http://localhost:5001
REACT_APP_AUDIO_URL=http://localhost:8082
REACT_APP_WS_URL=ws://localhost:5001
REACT_APP_VERSION=1.0.0

# Default settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# Feature flags
REACT_APP_ENABLE_VIDEO_STREAMING=true
REACT_APP_ENABLE_WEBRTC=true
REACT_APP_ENABLE_SOCIAL_MEDIA=true

# Backend connection settings
REACT_APP_BACKEND_TIMEOUT=5000
REACT_APP_RETRY_ATTEMPTS=3